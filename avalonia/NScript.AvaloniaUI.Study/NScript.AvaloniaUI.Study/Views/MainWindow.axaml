<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:NScript.AvaloniaUI.Study.ViewModels"
        xmlns:views="using:NScript.AvaloniaUI.Study.Views"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="NScript.AvaloniaUI.Study.Views.MainWindow"
        x:DataType="vm:MainWindowViewModel"
        Icon="/Assets/avalonia-logo.ico"
        Title="NScript.AvaloniaUI.Study">
    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>
  <StackPanel Orientation="Vertical">
    <views:MyGroupBox Margin="10" Background="White" GroupName="Tab示例">
      <views:MyUserControl Margin="0,0,0,10"></views:MyUserControl>
    </views:MyGroupBox>
    <views:MyGroupBox2 Margin="10" GroupName="Tab示例(MyGroupBox2)">
      <views:MyUserControl Margin="0,0,0,10"></views:MyUserControl>
    </views:MyGroupBox2>
  </StackPanel>
</Window>
